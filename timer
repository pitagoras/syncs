#bin/bash
now=`date +%k`
if [ 11 -gt $now -a $now -gt $12 ]; then
	phantomjs loadspeed.js  http://16.sync-playz.appspot.com/user1-aghbc/3.3-Weather_TARGET.htm
fi

if [ 1 -gt $now%3 ]; then
	phantomjs loadspeed.js http://16.sync-playz.appspot.com/user1-aghbc/3.3-daily_TARGET.htm
fi

phantomjs loadspeed.js http://16.sync-playz.appspot.com/user1-aghbc/3.3-everyBatch_TARGET.htm